{
  "name": "colosseum",
  "version": "1.0.0",
  "description": "A full stack web application for self betterment enthusiasts to find gyms of all types to improve on their physical capabilities",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "node server/index.js",
    "seed:dev": "node database/seeds/index.js dev",
    "seed:live": "node database/seeds/index.js live",
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack serve --mode=development",
    "dev:sass": "sass --watch server/public/scss:server/public",
    "dev:server": "nodemon server/index.js",
    "db:import": "bash scripts/db-import.bash",
    "test": "jest client/lib --watch"
  },
  "scriptComments": {
    "seed": "Seeds the gyms table in the database with dummy data",
    "db:import": "Drops the tables and import the schema and initial data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimmy-Vu/colosseum.git"
  },
  "keywords": [],
  "author": "Jimmy Vu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jimmy-Vu/colosseum/issues"
  },
  "homepage": "https://github.com/Jimmy-Vu/colosseum#readme",
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.13.6",
    "@react-spring/web": "^9.5.2",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/node": "^18.11.17",
    "@types/react-dom": "^18.0.9",
    "argon2": "^0.30.3",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.29.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mapbox-gl": "^2.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.19",
    "react-redux": "^8.0.4",
    "react-spinners": "^0.13.4",
    "unsplash-js": "^7.0.15"
  },
